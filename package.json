{
  "name": "imperial-lambda",
  "version": "1.0.0",
  "description": "Run massively concurrent scripts on AWS Lambda.",
  "scripts": {
    "build-fleet": "source ./configuration.sh && serverless deploy",
    "scan": "",
    "destroy-fleet": "source ./configuration.sh && serverless remove",
    "-": "-",
    "invoke": "source ./configuration.sh && serverless invoke -f imperial-lambda-fire -p example-queue-item.json",
    "local": "serverless invoke local -f imperial-lambda-fire -p example-queue-item.json",
    "logs": "source ./configuration.sh && serverless logs -f imperial-lambda-fire -t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saibotsivad/imperial-lambda.git"
  },
  "author": "saibotsivad",
  "license": "VOL",
  "bugs": {
    "url": "https://github.com/saibotsivad/imperial-lambda/issues"
  },
  "homepage": "https://github.com/saibotsivad/imperial-lambda#readme",
  "dependencies": {
    "dot-prop": "^5.2.0",
    "got": "^9.6.0",
    "p-all": "^2.1.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.580.0",
    "date-fns": "^2.8.1",
    "serverless": "^1.58.0"
  }
}
